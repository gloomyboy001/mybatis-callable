<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="db.table.user">

    <resultMap type="com.github.miemiedev.mybatis.callable.User" id="userMap">
        <id property="id" column="USER_ID" />
        <result property="name" column="USER_NAME"/>
        <result property="type" column="USER_TYPE"/>
    </resultMap>

    <resultMap type="java.util.HashMap" id="HashMap"></resultMap>

    <select id="queryTotalCountClassBar" statementType="CALLABLE">
        <![CDATA[
            {call test_proc(
                #{retCode,mode=OUT,jdbcType=INTEGER},
                #{acResult,mode=OUT,jdbcType=CURSOR,javaType=ResultSet, resultMap=HashMap},
                #{user.type},
                #{branchCode}
            )}
        ]]>
    </select>

</mapper>